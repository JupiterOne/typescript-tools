{
  "compilerOptions": {
    "noUnusedLocals": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "noImplicitThis": true,
    "alwaysStrict": true,
    "strictNullChecks": true,
    "strictFunctionTypes": true,
    "allowJs": false,
    "moduleResolution": "node",
    "target": "ESNext",
    "pretty": true,
    "inlineSourceMap": true,
    "inlineSources": true
  }
}
