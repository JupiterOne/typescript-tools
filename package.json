{
  "name": "@lifeomic/typescript-tools",
  "version": "1.0.0",
  "main": "./src/index.js",
  "repository": "https://bitbucket.org/lifeomic/typescript-tools",
  "author": "Development <development@lifeomic.com>",
  "license": "UNLICENSED",
  "scripts": {
    "lint": "eslint .",
    "precommit": "lint-staged",
    "test": "jest .",
    "test:ci": "yarn test"
  },
  "lint-staged": {
    "*.{ts,js,json,css,md}": [
      "prettier --write",
      "git add"
    ]
  },
  "eslintConfig": {
    "extends": [
      "plugin:@lifeomic/node/recommended"
    ]
  },
  "eslintIgnore": [
    "test/fixtures/**/*",
    "coverage/**"
  ],
  "bin": {
    "type-check": "./bin/type-check"
  },
  "dependencies": {
    "@babel/core": "^7.0.0-beta.49",
    "@lifeomic/lambda-tools": "^3.1.0",
    "fs-extra": "^6.0.1",
    "husky": "^0.14.3",
    "jest": "^23.1.0",
    "lint-staged": "^7.1.3",
    "prettier": "^1.13.4",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "tslint-config-prettier": "^1.13.0",
    "tslint-config-semistandard": "^7.0.0",
    "typescript": "^2.9.1"
  },
  "devDependencies": {
    "@lifeomic/dev-tools": "^4.2.3",
    "@lifeomic/eslint-plugin-node": "^1.1.1",
    "child-process-promise": "^2.2.1",
    "eslint": "^4.19.1",
    "eslint-plugin-jest": "^21.17.0",
    "uuid": "^3.2.1"
  },
  "resolutions": {
    "babel-core": "^7.0.0-bridge.0"
  },
  "dependents": [
    "lifeomic/lambda-typescript-jest-starter"
  ],
  "files": [
    "config/**/*",
    "src/**/*",
    "bin/**/*",
    "lambda-tools.js"
  ]
}
